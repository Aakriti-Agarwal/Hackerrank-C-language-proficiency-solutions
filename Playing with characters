#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    

    char ch;
    char str[100] , s[100];
    scanf("%c", &ch);
    scanf("\n");
    scanf("%s",&str);
    scanf("\n");
    scanf("%[^\n]%*c", s);
    printf("%c\n", ch);
    printf("%s\n", str);
    printf("%s\n", s);
    return 0;
}
